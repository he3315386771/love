<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <welcome-file-list>
        <welcome-file>love/index.html</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>love</servlet-name>
        <servlet-class>com.hejj.servlet.LoveServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>love</servlet-name>
        <url-pattern>/start.do</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>snowflake</servlet-name>
        <servlet-class>com.hejj.servlet.LoveServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>snowflake</servlet-name>
        <url-pattern>/snowflake.do</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>reminiscence</servlet-name>
        <servlet-class>com.hejj.servlet.LoveServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>reminiscence</servlet-name>
        <url-pattern>/reminiscence.do</url-pattern>
    </servlet-mapping>
</web-app>
